<Window x:Class="MFC_technic.IBP_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MFC_technic"
        mc:Ignorable="d"
        Title="Источники Бесперебойного Питания" Height="306" Width="392" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <DataGrid x:Name="TableIBP" ColumnWidth="*" Margin="0,10,0,0" Background="#FFE6E2E2" BorderBrush="{x:Null}" AutoGenerateColumns="False" Height="257" VerticalAlignment="Top" HorizontalAlignment="Center" Width="372">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Наименование" Binding="{Binding EquipmentName}"/>
                <DataGridTextColumn Header="Цена" Binding="{Binding Price}"/>
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
